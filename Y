local rangeLabel = Instance.new("TextLabel")  
rangeLabel.Size = UDim2.new(0, 45, 0, 20)  
rangeLabel.Position = UDim2.new(0, 5, 0, 102)  
rangeLabel.Text = t.range  
rangeLabel.BackgroundTransparency = 1  
